  "categories": [
    "Multimedia",
    "Desktop",
    "Productivity"
  ],
  "architecture": [
    "amd64"
  ],
  "compatibility": [
    "1.14.x"
  ],
  "cores": 2,
  "memory": 2768,
  "gpu_count": 0,
  "cpu_allocation_method": "Inherit",
  "uncompressed_size_mb": 8860
}