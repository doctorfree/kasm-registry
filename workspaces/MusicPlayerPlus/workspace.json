{
  "description": "MusicPlayerPlus is a character-based music player and self-hosted audio library management system",
  "docker_registry": "https://index.docker.io/v1/",
  "image_src": "mpplus.png",
  "name": "doctorwhen/kasm:mpplus",
  "friendly_name": "MusicPlayerPlus",
  "run_config": {
    "hostname": "kasm-mpplus"
