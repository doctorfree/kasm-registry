{
  "description": "Asciiville, Neovim, Spiderfoot, and Wing all in one workspace",
  "docker_registry": "https://index.docker.io/v1/",
  "image_src": "deluxe.png",
  "name": "doctorwhen/kasm:deluxe",
  "friendly_name": "Deluxe Desktop",
  "run_config": {
    "hostname": "kasm-deluxe"
  },
  "exec_config": {
     "first_launch": {
       "user":"root",
       "cmd":"bash -c 'echo \"kasm-user  ALL=(ALL) NOPASSWD: ALL\" >> /etc/sudoers'"
     }
  },
  "categories": [
    "Desktop",
    "Development",
    "Productivity"
  ],
  "architecture": [
    "amd64"
  ],
  "compatibility": [
    "1.14.x"
  ],
  "cores": 2,
  "memory": 5536,
  "gpu_count": 0,
  "cpu_allocation_method": "Inherit",
  "uncompressed_size_mb": 14900
}